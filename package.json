{
  "name": "ezhtml",
  "displayName": "EZHTML",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ezhtml.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ezhtml.insideNextTag",
        "title": "Move Inside Next Tag",
        "when": "editorLangId == markdown"
      },
      {
        "command": "ezhtml.insidePrevTag",
        "title": "Move Inside Previous Tag",
        "when": "editorLangId == markdown"
      }
    ],
    "keybindings": [
      {
          "command": "ezhtml.insideNextTag",
          "key": "ctrl+alt+.",
          "mac": "ctrl+cmd+."
      },
      {
        "command": "ezhtml.insidePrevTag",
        "key": "ctrl+alt+,",
        "mac": "ctrl+cmd+,"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}
